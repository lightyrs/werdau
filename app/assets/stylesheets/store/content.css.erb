  display: inline-block;
  height: 27px;
  line-height: 27px;
  background: url(<%= image_path 'store/red-button2-left.png' %>) no-repeat;
  padding: 0 0 0 10px;
}

.button.small-red:hover       { background-position: right -27px; }
