.button.large-red:hover span  { background-position: left  -40px; }
.button.large-red:active      { background-position: right -80px; }
.button.large-red:active span { background-position: left  -80px; }

.button.large-blue {
  display: inline-block;
  background: url(<%= image_path 'store/blue-button-right.png' %>) no-repeat top right;
  padding: 0 10px 0 0;

  font-size: 15px;
  text-transform: uppercase;
  color: #fff;
}

.button.large-blue span {
