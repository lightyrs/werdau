.button.small-red {
  display: inline-block;
  background: url(<%= image_path 'store/red-button2-right.png' %>) no-repeat top right;
  padding: 0 10px 0 0;

  font-size: 11px;
  color: #fff;
  text-transform: uppercase;
}

